	{
		List<MemberDTO> result = new ArrayList<MemberDTO>();
		
		// 데이터베이스 연결
		Class.forName("oracle.jdbc.driver.OracleDriver");
		Connection conn = DriverManager.getConnection("jdbc:oracle:thin:@211.238.142.165:xe"
				                                    , "scott"
				                                    , "tiger");
		
		// 쿼리문 준비 및 실행(select)
		String sql = "";
		
		
		
		return result;
		
	}
}
